<html>
  <head>
    <title>Artwork List</title>
    <style>
      body { font-family: Arial, sans-serif; background-color: #f4f4f4; color: #333; margin: 0; padding: 0; } .header {
      background-color: #2596be; color: white; text-align: center; padding: 10px 0; } .footer { background-color:
      #2596be; color: white; text-align: center; position: fixed; bottom: 0; width: 100%; padding: 10px 0; } h1 {
      margin-bottom: 20px; } ul { list-style-type: none; padding: 0; } li { border: 1px solid #ddd; background-color:
      #fff; padding: 15px; margin-bottom: 10px; } .container { padding-left: 40px; padding-right: 40px; } .price-capsule
      { display: inline-block; background-color: #2596be; color: white; padding: 5px 10px; border-radius: 15px;
      font-weight: bold; } .back-button { padding: 10px 15px; background-color: #2596be; color: white; text-decoration:
      none; border-radius: 5px; font-size: 14px; display: inline-block; margin-bottom: 20px; } .back-button:hover {
      background-color: #1d7b9d; }
    </style>
  </head>
  <body>

    <div class="header">
      <h2>Art Gallery</h2>
    </div>
    <div class="container">

      <div style="margin-top: 10px">
        <a href="javascript:history.back()" class="back-button">Go Back</a>
      </div>
      <h1>Art Works</h1>

      <ul>
        {{message}}
        {{#each artworks}}
          <li>
            <div>
              <h3>{{this.TITLE}}</h3>
              <p>Year: {{this.YEARCREATED}}</p>
              <p>Medium: {{this.MEDIUM}}</p>
              <p>Description: {{this.DESCRIPTION}}</p>
              <p class="price-capsule">$ {{this.PRICE}}</p>
            </div>
          </li>
        {{/each}}
      </ul>
    </div>

    <div class="footer">
      <p>&copy; 2023 Art Gallery. All rights reserved.</p>
    </div>
  </body>
</html>